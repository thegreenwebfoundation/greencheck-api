{
  "name": "thegreenwebfoundation/api",
  "description": "The Green Web Foundation API ",
  "authors": [
    {
      "name": "Arend-Jan Tetteroo",
      "email": "aj@arendjantetteroo.nl"
    }
  ],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/publicsuffix"
    },
    {
      "type": "path",
      "url": "../../packages/greencheck"
    },
    {
      "type": "vcs",
      "url": "https://github.com/thegreenwebfoundation/enqueue"
    },
    {
      "type": "vcs",
      "url": "https://github.com/thegreenwebfoundation/enqueue-bundle"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1.0",
    "doctrine/data-fixtures": "dev-master",
    "doctrine/doctrine-bundle": "~1.2",
    "doctrine/orm": "~2.3",
    "enqueue/amqp-ext": "^0.9.2",
    "enqueue/amqp-lib": "^0.9.8",
    "enqueue/enqueue-bundle": "dev-rabbit-lazy-queues",
    "enqueue/enqueue": "dev-rabbit-lazy-queues",
    "gedmo/doctrine-extensions": "~2.3",
    "liuggio/statsd-client-bundle": "*@dev",
    "predis/predis": "*",
    "sentry/sentry-symfony": "^3.0",
    "symfony/console": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "^4.0",
    "symfony/monolog-bundle": "~3.3",
    "symfony/polyfill-php73": "^1.11",
    "symfony/web-server-bundle": "^4.0",
    "symfony/yaml": "^4.0",
    "thegreenwebfoundation/greencheck": "*@dev",
    "thegreenwebfoundation/publicsuffix": "*@dev"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "roave/security-advisories": "dev-master",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "discard-changes": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.2.*"
    }
  }
}
